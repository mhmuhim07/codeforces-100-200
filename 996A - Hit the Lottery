#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define fo(i,a,n) for(ll i=a;i<n;i++)
#define all(v) (v).begin(),(v).end()
#define test int t; cin>>t; for(int no = 1;no<=t;no++)
#define pb push_back
#define endl '\n'
#define ff first
#define ss second
#define fast ios_base::sync_with_stdio(false);cin.tie(0),cout.tie(0);
void solve()
{
    ll n;
    cin>>n;
    ll sum =0;
    sum+=n/100;
    n=n%100;
    sum+=n/20;
    n=n%20;
    sum+=n/10;
    n=n%10;
    sum+=n/5;
    n=n%5;
    sum+=n;
    cout<<sum<<endl;


}
int main()
{

    fast;

    int t=1;
    //cin >> t;
    while(t--)
    {
        solve();
    }

    return 0;
}

